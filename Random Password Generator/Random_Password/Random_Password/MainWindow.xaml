<Window x:Class="Random_Password.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Random_Password"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="xgenerate_button" Content="generate" HorizontalAlignment="Left" Margin="112,178,0,0" FontSize="20" VerticalAlignment="Top" Width="118" Height="44" Click="Button_Click"/>
        <TextBox x:Name="xpassword_box" HorizontalAlignment="Left" Height="38" Margin="90,128,0,0" TextWrapping="Wrap" Text="Password" FontSize="15" VerticalAlignment="Top" Width="158"/>
        <ListBox x:Name="xpassword_history_box" HorizontalAlignment="Left" Height="187" Margin="410,128,0,0" VerticalAlignment="Top" Visibility="Collapsed" Width="146" SelectionChanged="xpassword_history_SelectionChanged"/>
        <Button x:Name="xhistoryshower_button" Content="History" HorizontalAlignment="Left" Margin="292,128,0,0" VerticalAlignment="Top" Width="100" Height="40" Click="xhistoryshower_Click"/>


    </Grid>
</Window>
